# Warehouse Configuration
WAREHOUSE=DUCKDB              # DUCKDB | SNOWFLAKE | BIGQUERY
DUCKDB_PATH=.data/local.duckdb

# Snowflake (optional)
SNOWFLAKE_ACCOUNT=
SNOWFLAKE_USER=
SNOWFLAKE_PASSWORD=
SNOWFLAKE_WAREHOUSE=
SNOWFLAKE_DATABASE=
SNOWFLAKE_SCHEMA=
SNOWFLAKE_ROLE=

# BigQuery (optional)
BQ_PROJECT_ID=
BQ_DATASET=
